{
  "name": "func-client",
  "type": "module",
  "version": "1.0.0-alpha.87",
  "license": "MIT",
  "description": "Functional programming oriented client for TS and JS, with strong typing and tree-shaking capabilities",
  "homepage": "https://paul-thebaud.github.io/func-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/paul-thebaud/func-client.git"
  },
  "bugs": {
    "url": "https://github.com/paul-thebaud/func-client/issues"
  },
  "keywords": [
    "typescript",
    "javascript",
    "functional programming",
    "model",
    "api",
    "json:api",
    "rest"
  ],
  "sideEffects": false,
  "files": [
    "dist/*",
    "core.d.ts",
    "http.d.ts",
    "json.d.ts",
    "jsonapi.d.ts",
    "jsonrest.d.ts",
    "blueprints.d.ts",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "main": "./dist/index.umd.cjs",
  "exports": {
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./json": {
      "import": "./dist/json.js",
      "require": "./dist/json.cjs"
    },
    "./http": {
      "import": "./dist/http.js",
      "require": "./dist/http.cjs"
    },
    "./jsonapi": {
      "import": "./dist/jsonapi.js",
      "require": "./dist/jsonapi.cjs"
    },
    "./jsonrest": {
      "import": "./dist/jsonrest.js",
      "require": "./dist/jsonrest.cjs"
    },
    "./blueprints": {
      "import": "./dist/blueprints.js",
      "require": "./dist/blueprints.cjs"
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "dev": "vite --config vite.config.js",
    "build:pre-clear": "rimraf ./dist ./dist-temp",
    "build:lib:es": "BUILD_FORMAT=es vite build --config vite.config.js",
    "build:lib:cjs": "BUILD_FORMAT=cjs vite build --config vite.config.js",
    "build:lib:umd": "BUILD_FORMAT=umd vite build --config vite.config.js",
    "build:lib": "concurrently \"pnpm build:lib:es\" \"pnpm build:lib:cjs\" \"pnpm build:lib:umd\"",
    "build:declaration": "tsc --declaration --emitDeclarationOnly --declarationDir dist-temp/ && tsc-alias",
    "build:declaration:bundle": "rollup -c rollup.dts.config.js",
    "build:post-clear": "rimraf \"./dist/index.@(js|cjs|d.ts)?(.map)\" && rimraf ./dist-temp",
    "build": "pnpm build:pre-clear && pnpm build:lib && pnpm build:declaration && pnpm build:declaration:bundle && pnpm build:post-clear",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "rimraf": "^4.4.0",
    "rollup": "^3.18.0",
    "rollup-plugin-dts": "^5.2.0",
    "tsc-alias": "^1.8.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": ">=16"
  }
}
