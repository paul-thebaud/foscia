{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "strict": true,
    "removeComments": false,
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "alwaysStrict": false,
    "baseUrl": "../..",
    "paths": {
      "func-model/core": [
        "src/core.ts"
      ],
      "func-model/json-api": [
        "src/json-api.ts"
      ]
    }
  }
}
