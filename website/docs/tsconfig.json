{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "strict": true,
    "removeComments": false,
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "alwaysStrict": false,
    "jsx": "preserve",
    "baseUrl": "..",
    "paths": {
      "foscia/core": [
        "dist/core.d.ts"
      ],
      "foscia/http": [
        "dist/http.d.ts"
      ],
      "foscia/json": [
        "dist/json.d.ts"
      ],
      "foscia/jsonapi": [
        "dist/jsonapi.d.ts"
      ],
      "foscia/jsonrest": [
        "dist/jsonrest.d.ts"
      ],
      "foscia/blueprints": [
        "dist/blueprints.d.ts"
      ]
    }
  }
}
